<h3><%= t('view.status') + t('view.edit') %></h3>
<%= form_with(model: @health_interview, local: true) do |f| %>

<p>No.<%= @health_interview.id %></p>

  <%= f.fields_for :guide_status do |s| %>
    <div class="field">
      <%= s.label t('view.status') %>
      <%= s.select :status, GuideStatus.statuses.keys %>
    </div>
  <% end %>

  <div class="field">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to t('view.index'), :back %>